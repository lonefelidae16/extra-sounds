{
  "schemaVersion": 1,
  "id": "extrasounds-logics",
  "version": "${version}",
  "name": "ExtraSounds base logics",
  "description": "Adds more sounds to Minecraft UI.",
  "authors": [
    "stashymane",
    "lonefelidae16"
  ],
  "contact": {
    "homepage": "${mod_page}",
    "sources": "${mod_sources}",
    "issues": "${mod_issues}"
  },
  "license": "${mod_license}",
  "icon": "assets/extrasounds/icon.png",
  "environment": "client",
  "entrypoints": {
    "sound-categories": [
      "dev.stashy.extrasounds.logics.Mixers"
    ]
  },
  "mixins": [
    "extrasounds.logics.mixins.json",
    "extrasounds.compat.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "minecraft": "*",
    "fabric-resource-loader-v0": "*",
    "soundcategories": "*"
  },
  "accessWidener": "extrasounds.accesswidener"
}
