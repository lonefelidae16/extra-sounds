{
  "schemaVersion": 1,
  "id": "extrasounds",
  "version": "${version}",
  "name": "ExtraSounds",
  "description": "Adds more sounds to Minecraft UI.",
  "authors": [
    "stashymane",
    "lonefelidae16"
  ],
  "contact": {
    "homepage": "https://artifactory.kow08absty.com/ui/native/fabricmc/dev/stashy/extra-sounds/",
    "sources": "https://github.com/lonefelidae16/extra-sounds",
    "issues": "https://github.com/lonefelidae16/extra-sounds/issues"
  },
  "license": "CC0-1.0",
  "icon": "assets/extrasounds/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.stashy.extrasounds.ExtraSounds"
    ],
    "extrasounds": [
      "dev.stashy.extrasounds.mapping.VanillaGenerator::generator"
    ],
    "sound-categories": [
      "dev.stashy.extrasounds.Mixers"
    ]
  },
  "mixins": [
    "extrasounds.mixins.json",
    "extrasounds.compat.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "minecraft": "~1.20.5",
    "fabric-resource-loader-v0": "*",
    "soundcategories": "*"
  },
  "accessWidener": "extrasounds.accesswidener"
}
